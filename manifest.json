{
    "manifest_version": 3,
    "name": "DofusBook GSheets Exporter",
    "version": "1.0",
    "description": "Exportez vos stuffs Dofus book en csv vers un Google Sheets plus facilement !",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://www.dofusbook.net/*"],
    "action": {
        "default_popup": "html/popup.html",
        "default_title": "DofusBook GSheets Exporter"
      }
  }